{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "React boilerplate",
  "main": "index.js",
  "scripts": {
    "start-app-dev": "webpack-dev-server --inline --progress --colors",
    "start-server-dev": "nodemon --exec babel-node ./server",
    "build-app-dev": "webpack --progress --colors",
    "build-dev-watch": "webpack --progress --colors --watch",
    "clean-client": "rm -rf build && mkdir build",
    "clean-server": "rm -rf ./server/build && mkdir server/build",
    "clean-all": "npm run clean-client && npm run clean-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Maxime Navet",
  "license": "ISC",
  "dependencies": {
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-router": "^2.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "html-webpack-plugin": "^2.22.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "write-file-webpack-plugin": "^3.1.8"
  }
}
